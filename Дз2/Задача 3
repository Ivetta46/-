#include <iostream>
#include <vector>

int main() {
    int n;
    std::cout << "Enter size of array: " << std::endl;
    std::cin >> n;
    std::vector<int> v(n);
    for(auto i = 0u; i < n; i++) {
        std:: cin >> v[i];
    }

    for(int i=1; i < n; i++) {
        for (unsigned int j = i; j > 0 && v[j - 1] > v[j]; j--) {
            std::swap(v[j - 1], v[j]);
        }
    }
    std::cout << "Sorted array: " << std::endl;
    for(int i = 0; i < n; i++){
        std::cout <<  v[i] << ' ' ;
    }
}
