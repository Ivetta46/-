#include <iostream>

template<typename Func, typename... Types>
void print(Func f, Types... arguments) {
    std::cout<< "Result is: " << f << std::endl;
}


template<typename... Types>
auto foldSum(Types... s)
{
    return (... + s); // ((s1 + s2) + s3) ...
}

int main() {
//    foldSum =  (int f)(int, int, int);
    print(foldSum(3, 5.5, 4));
}
