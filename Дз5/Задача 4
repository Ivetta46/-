Ошибка линкера с обнаружением многократно определенного внешнего символа:
//main.cpp
#include "List.h"

void main()
{
}

//List.h
#ifndef _LIST_H_
#define _LIST_H_
const char* assetType[] = {"menu", "products", "price"};
void func();
#endif

//List.c
#include "List.h"

void func()
{
    int i;
}

Ошибка линкера с неразрешенным внешним символом:
//main.cpp
#include "file.h"

int main()
{
   int a = GetIt();
   return 0;
}

//file.h
static int GetIt();

//file.c
#include "file.h"

static int GetIt()
{
   return 1;
}
